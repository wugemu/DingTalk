    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/ui_common_cell_pressed_bg_color" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/ui_common_cell_bg_color" />
        </shape>
    </item>
</selector>